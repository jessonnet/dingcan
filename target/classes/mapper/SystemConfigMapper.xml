<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.canteen.mapper.SystemConfigMapper">

    <select id="selectByConfigKey" parameterType="java.lang.String" resultType="com.canteen.entity.SystemConfig">
        SELECT * FROM system_config WHERE config_key = #{configKey}
    </select>

</mapper>
